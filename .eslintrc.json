{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "func-names": "off",
    "prettier/prettier": "error",
    "import/prefer-default-export": "off"
  }
}
